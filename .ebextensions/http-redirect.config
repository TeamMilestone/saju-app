Resources:
  AWSEBV2LoadBalancerListenerHTTPRedirect:
    Type: AWS::ElasticLoadBalancingV2::ListenerRule
    Properties:
      ListenerArn:
        Ref: AWSEBV2LoadBalancerListener
      Priority: 1
      Conditions:
        - Field: host-header
          Values:
            - saju.cvvcv.click
      Actions:
        - Type: redirect
          RedirectConfig:
            Protocol: HTTPS
            Port: 443
            Host: "#{host}"
            Path: "/#{path}"
            Query: "#{query}"
            StatusCode: HTTP_301